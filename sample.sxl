/**
 * Sample function: adds two integers
 */
function add( x : int, y : int ) : int {
	let z : int = x + y;
	z;
}

// Print numbers 0 to 9
let i : int = 0;
while ( i < 10 ) {
	write i;
	set i <- i + 1;
	if ( i == 10 ) {
		let m : string = "Done!";
		write m;
	}
}

// Check generated syntax tree, to see how expressions obey operator precedence
set i <- (i * 2) - (4 + (3/2));

// Exit
halt 0;